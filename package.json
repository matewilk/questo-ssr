{
  "name": "questo-front-ssa",
  "version": "0.0.1",
  "description": "Questo front-end with SS rendering",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch build --exec \"node --inspect=localhost:9228 build/bundle.js\"",
    "dev:build-server": "webpack --config webpack.server.js --watch",
    "dev:build-client": "webpack --config webpack.client.js --watch",
    "test": "jest",
    "start": "npm-run-all -p start:* -s run:server",
    "run:server": "nodemon --watch build --exec \"node --inspect=localhost:9228 build/bundle.js\"",
    "start:build-server": "webpack --config webpack.server.js",
    "start:build-client": "webpack --config webpack.client.js",
    "docker:dev:start": "docker-compose -f docker-compose.dev.yml up",
    "docker:dev:stop": "docker-compose -f docker-compose.dev.yml down"
  },
  "keywords": [
    "react",
    "redux",
    "ssr",
    "graphql"
  ],
  "author": "m.wilk",
  "license": "ISC",
  "dependencies": {
    "@apollo/client": "^3.5.6",
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@babel/runtime": "^7.16.0",
    "@reduxjs/toolkit": "^1.7.1",
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.2",
    "@types/http-proxy-middleware": "^0.19.3",
    "@types/jest": "^27.0.2",
    "@types/node": "^13.1.8",
    "@types/react": "^16.9.18",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-config": "^5.0.1",
    "@types/react-router-dom": "^5.1.3",
    "@types/serialize-javascript": "^1.5.0",
    "axios": "^0.22.0",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.0.6",
    "babel-preset-edge": "^5.5.1",
    "dotenv": "^10.0.0",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.1",
    "jest": "^27.2.4",
    "nodemon": "^2.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.1.3",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.1.2",
    "react-spring": "^9.4.2",
    "react-test-renderer": "^17.0.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "serialize-javascript": "^6.0.0",
    "subscriptions-transport-ws": "^0.11.0",
    "webpack": "^5.58.0",
    "webpack-dev-server": "^4.3.1",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0",
    "ws": "^7.5.5"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.16.0",
    "@graphql-tools/schema": "^8.3.1",
    "@testing-library/react": "^12.1.2",
    "@types/dotenv-webpack": "^7.0.3",
    "@types/ws": "^8.2.2",
    "apollo-server": "^3.6.2",
    "apollo-server-express": "^3.6.2",
    "core-js": "^3.21.0",
    "dotenv-webpack": "^7.0.3",
